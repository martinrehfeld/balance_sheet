<h1>New entry_type</h1>

<% form_for(@entry_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :cumulative %><br />
    <%= f.check_box :cumulative %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', entry_types_path %>
